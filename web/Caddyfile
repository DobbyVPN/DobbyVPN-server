
:443 {

    log {
        output stdout
    }

    @cloakPath {
        path /special-url
    }

    handle @cloakPath {
	reverse_proxy @cloakPath 127.0.0.1:8443
        #respond "URL FOUND-CLOAK" 200
    }	

    handle {
        root * /usr/share/caddy
	file_server 
        #respond "FILE SERVER" 200
    }

   }

